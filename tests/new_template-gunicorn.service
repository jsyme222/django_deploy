[Unit]
Description=gunicorn daemon
After=network.target

[Service] 
User=jsyme
#  user = (user, Required, default: $USER)

Group=www-data
#  group = www-data(default: www-data)
 
WorkingDirectory=/home/jsyme/projects/pycharm/django_deploy
#  root_dir = /home/$USER/projects/project_name/app

ExecStart=/home/jsyme/projects/pycharm/.env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/jsyme/projects/pycharm/django_deploy/django_deploy.sock django_deploy.wsgi:application
#  sock_path = /home/jsyme/projects/project_name/app/app.sock
#  project_name = app

[Install]
WantedBy=multi-user.target
